    "ui:add": "npx shadcn-ui@latest add"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "*",
    "@repo/tailwind-config": "*",
    "@repo/tsconfig": "*",
    "@types/node": "^20.9.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "autoprefixer": "^10.4.16",
    "delete-cli": "^0.1.3",
